            <div class="col-xs-1 liv-gps">
            <i class="fa fa-crosshairs" aria-hidden="true" ng-if="showGPS"></i>
            </div>
          </div>
          
          <div class="row" id="liv-input-row" ng-if="showLocality">
          	<div class="col-xs-12">
          		<div class="input-group">         
	              <input type="text" class="form-control" placeholder="Enter Location" id="address" ng-model="locality">
	              <span class="input-group-addon" id="liv-input-search" ng-click="getLocalitiesAvailability(locality)"><i class="fa fa-search"></i></span>
	            </div>
          	</div>
          </div>    
        </div>
      </nav>
