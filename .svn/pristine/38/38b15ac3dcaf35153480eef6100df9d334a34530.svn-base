												<label class="con">Hotel/Accomodation <input
													type="checkbox" ng-model="accommodationCheckBox"
													ng-click="discard('accomodation')"
													ng-change="accommodationCheckBox || selectForm('accommodationCheckBox');checkedOrNotAccomodation(accommodationCheckBox);accomodationValid(accommodationCheckBox);">
													<span class="checkmark"></span>
												</label>
											</div>


											<div class="col-lg-2 col-xs-6">

												<label class="con">Taxi <input type="checkbox"
													ng-model="taxiCheckBox" ng-click="discard('taxi')"
													ng-change="taxiCheckBox || selectForm('taxiCheckBox');checkedOrNotCab(taxiCheckBox);accomodationValid(taxiCheckBox);">
													<span class="checkmark"></span>
												</label>
											</div>

											<div class="col-lg-2 col-xs-6">

												<label class="con">Flight <input type="checkbox"
