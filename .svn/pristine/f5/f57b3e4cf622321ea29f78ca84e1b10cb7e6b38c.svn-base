@import "estimate-summary";
@leaf:#8dc634;

/* fontsize style*/
@font_14:14px;
@font_16:16px;
@font_18:18px;

/* font style*/
@font_style:'Glegoo', serif; 

header{
    background-color: @leaf;
    height: 80px;
    width: 100%;
    nav{
                   
        .row{
            margin-top: 10px;
            color: white;
            font-size: @font_16;
            font-style: @font_style;                                    

        .liv-head-btm-text{                         
                font-size: 12px;
                   }
                     .liv-head-heading{
                        text-align: center;
                        margin-right:50px; 
                               }
                        .liv-head-back-btm{
                            float: left;
                           }
                        }              
                    }
                }
.liv-head-pref{
    margin-top: 50px;
    margin-bottom: 50px;
    color: black ;
}
.liv-head-sal{
    p{
        color: orangered;
    }
}
section
    {
       input {
          border: 0;
          outline: 0;
          width: 100%;
          background: transparent;
          border-bottom: 1px solid #CBCBCB;
          font-size: 20px;
        }   
}
.liv-head-incom{
     margin-top: 30px;
    p{
        color: orangered;
    }
}
.liv-head-estimate{
    margin-top: 30px;
    p{
        color: orangered;
    }
}
