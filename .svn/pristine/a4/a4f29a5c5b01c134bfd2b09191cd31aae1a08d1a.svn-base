            <section class="container-bg">

                <header>
                    <h1 class="p-tb-md m-n p">New Travel Request</h1>
                </header>
                <section class="main-bg">

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <section>
                                <ul class="nav nav-tabs text-center">
                                    <li ng-class="tabClassPersonal" class="active" ng-click="colorChangePersonal()"><a data-toggle="tab" href="#personalDetails">Personal Details</a></li>
                                    <li ng-class="tabClassRequest" ng-click="colorChangeRequest()"><a data-toggle="tab" href="#reqDetails">Request Details</a></li>
                                    <li ng-class="tabClassConfirm" ng-click="colorChangeConfirm()"><a data-toggle="tab" href="#confirm">Confirm</a></li>
                                </ul>
                            </section>
                        </div>
                        <section class="tab-content panel-body">
                            <div class="progress progress-xs m-t-md">
                                <div ng-class="progressClass1" style="width: 33.3333%;"></div>
                                <div ng-class="progressClass2" style="width: 33.3333%;"></div>
                                <div ng-class="progress-bar" style="width: 33.3333%;"></div>
                            </div>
                            <section class="tab-content panel-body">

                                <div id="personalDetails" class="tab-pane in active">
                                    <form action="" name="personal">

                                        <!-- Cerating a contianer having padding-->

                                        <section>
                                            <!-- Radio button selector i.e mandatory for selecting booking travel for self
                                                                            or others-->
                                            <!-- ======= Start of radio======== -->




                                            <div class="input">
                                                <header class="tp-radio-pad">
                                                    Please select from one of the following options*
                                                </header>
                                                <label class="radio-inline">
                                                        
                                                    <input type="radio" name="self-other">
                                                        <i></i>
                                                        Self
                                                </label>
                                                <label class="radio-inline m-l-lg">
                                                    <input type="radio" name="self-other">
                                                        <i></i>
                                                        Other Employee
                                                </label>
                                            </div>

                                            <!--============Ends of radio ============= -->


                                            <!--========= Start of input field section
                                                                                         Starts here       ========== -->
                                            <div class="row tp-field-heading">


                                                <div class="col-xs-12 col-md-6">
                                                    <label>Employee Name*</label>
                                                    <input type="text" class="form-control input-sm" name="personName" placeholder="Employee Name" ng-model="empObj.employeeName"
                                                        ng-minlength="4" ng-maxlength="50" ng-pattern="/^[a-zA-Z ][^\s]*$/" required
                                                    />
                                                    <div ng-messages="personal.personName.$error" ng-if="personal.personName.$touched" ng-class="errMsg">
                                                        <span ng-message="required">{{::err.data.validations.employeeName.required}}</span>
                                                        <span ng-message="pattern">{{::err.data.validations.employeeName.pattern}}</span>
                                                        <span ng-message="minlength">{{::err.data.validations.employeeName.minlength}}</span>
                                                        <span ng-message="maxlength">{{::err.data.validations.employeeName.maxlength}}</span>
                                                    </div>

                                                </div>



                                                <div class="col-xs-12 col-md-6">

                                                    <label>Employee Contact*</label>
                                                    <input type="text" class="form-control input-sm" placeholder="Employee Contact" name="personContact" ng-model="empObj.employeeMobileNumber"
                                                        ng-minlength="10" ng-maxlength="10" ng-pattern="/^[0-9][^\s]*$/" required
                                                    />
                                                    <div style="position : absolute;" ng-messages="personal.personContact.$error" ng-if="personal.personContact.$touched" ng-class="errMsg">
                                                        <span ng-message="required">{{::err.data.validations.employeeContact.required}}</span>
                                                        <span ng-message="pattern">{{::err.data.validations.employeeContact.pattern}}</span>
                                                        <span ng-message="minlength">{{::err.data.validations.employeeContact.minlength}}</span>
                                                        <span ng-message="maxlength">{{::err.data.validations.employeeContact.maxlength}}</span>
                                                    </div>
                                                </div>
                                                <!-- right side column ends-->

                                            </div>


                                            <div class="row tp-field-heading">
                                                <div class="col-xs-12 col-md-6">
                                                    <label>Employee Location*</label>
                                                    <select class="form-control input-sm" ng-model="empObj.employeeLocation" name="personLocation" ng-options="city for city in personalDetails.employeeLocation"
                                                        required>
                                                                            <option value="" disabled selected>--Select Location--</option>
                                                                           <!--  <option value="1">{{err.data.personalDetails.employeeLocation[0]}}</option>
                                                                            <option value="2">{{err.data.personalDetails.employeeLocation[1]}}</option> -->
                                                                        </select>
                                                    <div ng-messages="personal.personLocation.$error" ng-if="personal.personLocation.$touched" ng-class="errMsg">
                                                        <span ng-message="required">{{::err.data.validations.employeeLocation.required}}</span>
                                                    </div>

                                                </div>

                                                <div class="col-xs-12 col-md-6">
                                                    <label>Approver Name*</label>
                                                    <select class="form-control input-sm" ng-model="empObj.approverName" name="personApproverName" placeholder="Approver Name"
                                                        ng-options="person for person in personalDetails.approverName" required>
                                                                            <option value="" disabled selected>--Select Name--</option>
                                                                            <!-- <option value="1">{{err.data.personalDetails.approverName[1]}}</option>  -->
                                                                        </select>
                                                    <div ng-messages="personal.personApproverName.$error" ng-if="personal.personApproverName.$touched" ng-class="errMsg">

                                                        <span ng-message="required">{{::err.data.validations.approverName.required}}</span>

                                                    </div>
                                                </div>
                                            </div>



                                            <div class="row tp-field-heading">
                                                <div class="col-xs-12 col-md-6">

                                                    <label>Project Name*</label>
                                                    <input type="text" class="form-control input-sm" placeholder="Project Name" name="personProjectName" ng-model="empObj.projectName"
                                                        required />
                                                    <div ng-messages="personal.personProjectName.$error" ng-if="personal.personProjectName.$touched" ng-class="errMsg">
                                                        <span ng-message="required">{{::err.data.validations.projectName.required}}</span>

                                                    </div>

                                                </div>

                                                <div class="col-xs-12 col-md-6">

                                                    <label>Project Code/Charge Code*</label>
                                                    <input type="text" class="form-control input-sm" placeholder="Project Code" name="personProjectCode" ng-model="empObj.projectCode"
                                                        required />
                                                    <div ng-messages="personal.personProjectCode.$error" ng-if="personal.personProjectCode.$touched" ng-class="errMsg">
                                                        <span ng-message="required">{{::err.data.validations.projectCode.required}}</span>

                                                    </div>
                                                </div>
                                            </div>


                                            <!-- Start of buttons section
                                                                              Starts here  -->
                                            <footer>
                                                <nav aria-label="...">
                                                    <ul class="pager">
                                                        <li class="next"><a data-toggle="tab" ng-click="clickhi()">Next</a></li>
                                                    </ul>
                                                </nav>
                                            </footer>



                                        </section>



                                        <!-- ==========-end of container section============= -->

                                    </form>
                                </div>



                                <!-- second tab-->

                                <div id="reqDetails" class=" tab-pane">
                                    <!-- ============================================
                                                    |    start of Request Details form section      |
                                                    ==============================================-->
                                    <section>
                                        <!-- <button ng-click="send()">qqq</button> -->
                                        <div ng-form="chkbox" class="row">
                                            <div class="col-lg-3 col-xs-6">
                                                <input type="checkbox" ng-model="accommodationCheckBox" ng-change="accommodationCheckBox || selectForm('accommodationCheckBox');checkedOrNotAccomodation(accommodationCheckBox)">
                                                <label>Hotel/Accomodation</label>
                                            </div>


                                            <div class="col-lg-3 col-xs-6">
                                                <input type="checkbox" ng-model="taxiCheckBox" ng-change="taxiCheckBox || selectForm('taxiCheckBox');checkedOrNotCab(taxiCheckBox)">
                                                <label>Taxi</label>
                                            </div>

                                            <div class="col-lg-3 col-xs-6">
                                                <input type="checkbox" ng-model="flightCheckBox" ng-change="flightCheckBox || selectForm('flightCheckBox');checkedOrNotFlight(flightCheckBox)">
                                                <label>Flight</label>
                                            </div>

                                            <div class="col-lg-3 col-xs-6">
                                                <input type="checkbox" ng-model="forexCheckBox" ng-change="forexCheckBox || selectForm('forexCheckBox');checkedOrNotForex(forexCheckBox)">
                                                <label>Forex</label>
                                            </div>
                                        </div>

                                        <div>
                                            <!-- ===============================================================
                                                                |						panel group || accordians               |
                                                                ================================================================- -->

                                            <div class="panel-group">
                                                <!-- ==============Hotel / Accomodation panel/accordion============ -->
                                                <form action="" name="accomodationForm" ng-if="accommodationCheckBox">
                                                    <div class="panel panel-default">
                                                        <!-- =============Panel Heaading============= -->
                                                        <div ng-class="accomodationHeading" ng-click="collapseAccordion('accomodationAccordion')">
                                                            <h4 class="panel-title p">
                                                                <a data-toggle="collapse" data-parent="#accordion">
                                                                            Hotel/Accommodation
                                                                </a>
                                                            </h4>

                                                        </div>

                                                        <div id="collapseOne" ng-class="accomodationAccordion" class="">
                                                            <div class="panel-body">
                                                                
                                                                    <div class="row tp-field-heading">
                                                                        <div class="col-xs-12 col-md-6">
                                                                            <label>Country*</label>
                                                                            <select class="form-control" name="accommodationCountry" ng-model="accomodationObj.country" ng-options="country for country in accomodation.country"
                                                                                required />
                                                                                 <option value="" disabled selected>--Select Country--</option>
                                                                             </select>
                                                                            <div ng-messages="accomodationForm.accommodationCountry.$error" ng-if="accomodationForm.accommodationCountry.$touched"
                                                                                ng-class="errMsg">
                                                                                <span ng-message="required">{{::err.data.validations.country.required}}</span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-xs-12 col-md-6">

                                                                            <label>City*</label>
                                                                            <!-- <input type="text" class="form-control input-sm" ng-model="accomodationObj.city" name="accommodationCity" required />

                                                                            <div ng-messages="accommodationForm.accommodationCity.$error" ng-if="accommodationForm.accommodationCity.$touched" ng-class="errMsg">
                                                                                <span ng-message="required">{{::err.data.validations.city.required}}</span>
                                                                            </div> -->
                                                                            <input type="text" class="form-control input-sm" ng-model="accomodationObj.city" name="accommodationCity" required />
                                                                            <div ng-messages="accomodationForm.accommodationCity.$error" ng-if="accomodationForm.accommodationCity.$touched" ng-class="errMsg">
                                                                               <span ng-message="required">{{::err.data.validations.city.required}}</span>
                                                                               
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row tp-field-heading">
                                                                        <div class="col-xs-12 col-md-6">
                                                                            <label>Check-in Date*</label>
                                                                            <div class="input-form">
                                                                                <!-- <input type="text" class="form-control" uib-datepicker-popup="{{dateFormat}}" ng-model="checkInDate" name="checkIndate" is-open="checkInDatePopup.opened" datepicker-options="checkInDateOptions" ng-required="true" close-text="Close" ng-click="OpencheckInDate()"
                                                                                                ng-change="ChangecheckOutMinDate(checkInDate)" placeholder="checkIn Date" />
                                                                                                <label ng-show="showMessages && addJobForm.checkIndate.$invalid" class="text-danger">
                                                                                                    checkIn Date is required.
                                                                                                </label> -->
                                                                                <input type="text" class="form-control" uib-datepicker-popup="{{dateFormat}}" ng-model="accomodationObj.checkInDate" name="accommodationCheckInDate" is-open="checkInDatePopup.opened"
                                                                                    datepicker-options="checkInDateOptions" ng-required="true"
                                                                                    close-text="Close" ng-click="OpencheckInDate()"
                                                                                    ng-change="ChangecheckOutMinDate(checkInDate)"
                                                                                    placeholder="Check in date" required />
                                                                                <label ng-show="showMessages && addJobForm.checkInDate.$invalid" class="text-danger">
                                                                                                    
                                                                                                </label>
                                                                            </div>
                                                                            <div ng-messages="accomodationForm.accommodationCheckInDate.$error" ng-if="accomodationForm.accommodationCheckInDate.$touched"
                                                                                ng-class="errMsg">
                                                                                <span ng-message="required">{{::err.data.validations.checkInDate.required}}</span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-xs-12 col-md-6">

                                                                            <label>Check-in Time*</label>
                                                                            <select class="form-control" ng-model="accomodationObj.checkInTime" name="accommodationCheckInTime" ng-options="interval for interval in intervals" required />
                                                                                                <!-- <option value="">==Select Time==</option> -->
                                                
                                                                                            </select>

                                                                            <div ng-messages="accomodationForm.accommodationCheckInTime.$error" ng-if="accomodationForm.accommodationCheckInTime.$touched"
                                                                                ng-class="errMsg">
                                                                                <span ng-message="required">{{::err.data.validations.checkInTime.required}}</span>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row tp-field-heading">
                                                                        <div class="col-xs-12 col-md-6">
                                                                            <label>Check-out Date*</label>
                                                                            <div class="input-form">
                                                                                <input type="text" class="form-control" uib-datepicker-popup="{{dateFormat}}" ng-model="accomodationObj.checkOutDate" name="accommodationCheckOutDate"
                                                                                    is-open="checkOutDatePopup.opened" datepicker-options="checkOutDateOptions"
                                                                                    ng-required="true" close-text="Close" ng-click="OpencheckOutDate()"
                                                                                    placeholder="Check-out Date" />

                                                                            </div>
                                                                            <div ng-messages="accomodationForm.accommodationCheckOutDate.$error" ng-if="accomodationForm.accommodationCheckOutDate.$touched"
                                                                                ng-class="errMsg">
                                                                                <span ng-message="required">{{::err.data.validations.checkOutDate.required}}</span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-xs-12 col-md-6">

                                                                            <label>Check-out Time*</label>
                                                                            <select class="form-control" ng-model="accomodationObj.checkOutTime" name="accommodationCheckOutTime" ng-options="interval for interval in intervals" required />
                                                                            </select>

                                                                            <div ng-messages="accomodationForm.accommodationCheckOutTime.$error" ng-if="accomodationForm.accommodationCheckOutTime.$touched"
                                                                                ng-class="errMsg">
                                                                                <span ng-message="required">{{::err.data.validations.checkOutTime.required}}</span>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row tp-field-heading">
                                                                        <div class="col-xs-12 col-md-6">
                                                                            <label>Budget Per Day*</label>
                                                                            <input type="text" class="form-control" placeholder="Budget Per Day" ng-model="accomodationObj.budget" name="accommodationBudgetPerDay" 
                                                                                ng-pattern="/^[0-9]*$/" required/>

                                                                            <div ng-messages="accomodationForm.accommodationBudgetPerDay.$error" ng-if="accomodationForm.accommodationBudgetPerDay.$touched"
                                                                                ng-class="errMsg">
                                                                                <span ng-message="pattern">{{::err.data.validations.budgetPerDay.pattern}}</span>
                                                                                <span ng-message="required">{{::err.data.validations.budgetPerDay.required}}</span>

                                                                            </div>
                                                                        </div>
                                                                        <div class="col-xs-12 col-md-6">

                                                                            <label>Currency*</label>
                                                                            <select class="form-control" ng-model="accomodationObj.currency" name="accommodationCurrency" ng-options="currency for currency in accomodation.currency" required/>
                                                                                                <option value="" disabled selected>--Select Currency--</option>
                                                                                            </select>

                                                                            <div ng-messages="accomodationForm.accommodationCurrency.$error" ng-if="accomodationForm.accommodationCurrency.$touched"
                                                                                ng-class="errMsg">

                                                                                <span ng-message="required">{{::err.data.validations.currency.required}}</span>

                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row tp-field-heading">
                                                                        <div class="col-xs-12 col-md-6">
                                                                            <label>Type of Room*</label>
                                                                            <select class="form-control" ng-model="accomodationObj.roomType" name="accommodationTypeOfRoom" ng-options="typeOfRoom for typeOfRoom in accomodation.typeOfRoom"
                                                                                required>
                                                                                                <option value="" disabled selected>--Select Type--</option>
                                                                                            </select>
                                                                            <div ng-messages="accomodationForm.accommodationTypeOfRoom.$error" ng-if="accomodationForm.accommodationTypeOfRoom.$touched"
                                                                                ng-class="errMsg">

                                                                                <span ng-message="required">{{::err.data.validations.typeOfRoom.required}}</span>

                                                                            </div>
                                                                        </div>
                                                                        <div class="col-xs-12 col-md-6">

                                                                            <label>Nearest Facility*</label>
                                                                            <select class="form-control input-sm" ng-model="accomodationObj.nearestFacility" name="accommodationNearestFacility" ng-options="nearestFacility for nearestFacility in accomodation.nearestFacility"
                                                                                required>
                                                                                                    <option value="" disabled selected>--Select facility--</option>
                                                                                                </select>

                                                                            <div ng-messages="accomodationForm.accommodationNearestFacility.$error" ng-if="accomodationForm.accommodationNearestFacility.$touched"
                                                                                ng-class="errMsg">

                                                                                <span ng-message="required">{{::err.data.validations.nearestFacility.required}}</span>

                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row tp-field-heading">
                                                                        <div class="col-xs-12 col-md-12">
                                                                            <label>Remark</label>
                                                                            <textarea class="form-control" placeholder="Remark" ng-model="accomodationObj.remarks"></textarea>
                                                                        </div>
                                                                    </div>
                                                                    
                                                                
                                                            </div>
                                                        </div>


                                                    </div>
                                                </form>
                                                <!-- =============end of Hotel/Accomodation panel/accordion===============- -->

                                                <!--===============Cab/Taxi panel/accordion ===============- -->
                                                <form action="" name="cabForm" ng-if="taxiCheckBox">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading" ng-click="collapseAccordion('taxiAccordion')">
                                                            <h4 class="panel-title p">
                                                                <a data-toggle="collapse" data-parent="#accordion">
                                                                            Cab/Taxi Request</a>
                                                            </h4>
                                                        </div>
                                                        <div id="collapseTwo" ng-class="taxiAccordion">
                                                            <div class="panel-body">
                                                                <div class="row tp-field-heading">
                                                                    <div class="col-xs-12 col-md-4">
                                                                        <label>Car Request Type*</label>
                                                                        <select class="form-control input-sm" ng-model="cabObj.taxiTravelType" name="cabCarRequestType" ng-options="carRequestType for carRequestType in taxi.carRequestType"
                                                                            required>
                                                                                        <option value="" disabled selected>--Select Type--</option>
                                                                                    </select>
                                                                        <div ng-messages="cabForm.cabCarRequestType.$error" ng-if="cabForm.cabCarRequestType.$touched" ng-class="errMsg">

                                                                            <span ng-message="required">{{::err.data.validations.carRequestType.required}}</span>

                                                                        </div>

                                                                    </div>
                                                                    <div class="col-xs-12 col-md-4">

                                                                        <label>City*</label>
                                                                        <input type="text" class="form-control input-sm" ng-model="cabObj.city" name="cabCity" placeholder="Enter City" required />

                                                                        <div ng-messages="cabForm.cabCity.$error" ng-if="cabForm.cabCity.$touched" ng-class="errMsg">

                                                                            <span ng-message="required">{{::err.data.validations.city.required}}</span>

                                                                        </div>


                                                                    </div>
                                                                    <div class="col-xs-12 col-md-4">

                                                                        <label>Pickup Point/Place*</label>
                                                                        <input type="text" class="form-control input-sm" placeholder="Pickup Point/Place" ng-model="cabObj.pickUpLocation" name="cabPickupPoint"
                                                                            required>
                                                                        <div ng-messages="cabForm.cabPickupPoint.$error" ng-if="cabForm.cabPickupPoint.$touched" ng-class="errMsg">

                                                                            <span ng-message="required">{{::err.data.validations.pickUpPoint.required}}</span>

                                                                        </div>


                                                                    </div>
                                                                </div>

                                                                <div class="row tp-field-heading">
                                                                    <div class="col-xs-12 col-md-4">
                                                                        <label>If other,Enter Address*</label>
                                                                        <input type="text" class="form-control input-sm" placeholder="Address" ng-model="cabObj.otherAddress" name="cabOtherAddress" required>
                                                                        <div ng-messages="cabForm.cabOtherAddress.$error" ng-if="cabForm.cabOtherAddress.$touched" ng-class="errMsg">

                                                                            <span ng-message="required">{{::err.data.validations.emailAddress.required}}</span>

                                                                        </div>
                                                                    </div>
                                                                    <div class="col-xs-12 col-md-4">

                                                                        <label>If Outstation, Enter City*</label>
                                                                        <input type="text" class="form-control input-sm" ng-model="cabObj.outstationCity" name="cabOutstationCity" placeholder="If outstation, enter city" required />

                                                                        <div ng-messages="cabForm.cabOutstationCity.$error" ng-if="cabForm.cabOutstationCity.$touched" ng-class="errMsg">

                                                                            <span ng-message="required">{{::err.data.validations.city.required}}</span>

                                                                        </div>


                                                                    </div>
                                                                    <div class="col-xs-12 col-md-4">
                                                                        <label>Drop Point/Place*</label>
                                                                        <input type="text" class="form-control input-sm" placeholder="Drop Location" ng-model="cabObj.dropLocation" name="cabDropPoint"
                                                                            required>
                                                                        <div ng-messages="cabForm.cabDropPoint.$error" ng-if="cabForm.cabDropPoint.$touched" ng-class="errMsg">

                                                                            <p ng-message="required">{{::err.data.validations.dropPoint.required}}</p>
                                                                            <span ng-message="required">{{::err.data.validations.dropPoint.required}}</span>
                                                                            <span ng-message="required">{{::err.data.validations.dropPoint.required}}</span>

                                                                        </div>


                                                                    </div>
                                                                </div>
                                                                <div class="row tp-field-heading">
                                                                    <div class="col-xs-12 col-md-4">
