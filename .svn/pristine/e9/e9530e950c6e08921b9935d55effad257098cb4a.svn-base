.con {
                -moz-user-select:none;
                -ms-user-select:none;
                -webkit-user-select:none;
                cursor:pointer;
                display:block;
                margin-bottom:12px;
                padding-left:35px;
                position:relative;
                user-select:none;
                input {
                                opacity:0;
                                position:absolute;
                                &:checked~.checkmark {
                                                background-color:@color2;
                                                after {
                                                                display:block;
                                                }
                                }
                }
                &:hover {
                                input~.checkmark {
                                                background-color:@color3;
                                }
                }
                .checkmark {
                                &:after {
                                                -ms-transform:rotate(45deg);
                                                -webkit-transform:rotate(45deg);
                                                border-width:0 3px 3px 0;
                                                border:solid @color0;
                                                height:@height2;
                                                left:7px;
                                                top:5px;
                                                transform:rotate(45deg);
                                                width:@width11;
                                }
                }
}
