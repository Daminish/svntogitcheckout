// Color variables (appears count calculates by raw css)
@color0: #ffffff; // Appears 6 times
@color1: #eeeeee; // Appears 3 times
@color2: #2196f3; // Appears 2 times
@color3: #cccccc; // Appears 2 times

// Width variables (appears count calculates by raw css)
@width0: 33.3333%; // Appears 1 time
@width1: 1100px; // Appears 1 time
@width2: 1090px; // Appears 1 time
@width3: 125px; // Appears 2 times
@width4: 360px; // Appears 1 time
@width5: 120px; // Appears 1 time
@width6: 20px; // Appears 2 times
@width7: 90px; // Appears 1 time
@width8: 50px; // Appears 1 time
@width9: 80px; // Appears 1 time
@width10: 100%; // Appears 2 times
@width11: 5px; // Appears 1 time
@width12: 8px; // Appears 1 time

// Height variables (appears count calculates by raw css)
@height0: 1070px; // Appears 1 time
@height1: 70px; // Appears 1 time
@height2: 10px; // Appears 1 time
@height3: 20px; // Appears 2 times
@height4: 80px; // Appears 1 time
@height5: 30px; // Appears 1 time
@height6: 60px; // Appears 1 time
@height7: 40px; // Appears 2 times
@height8: 100%; // Appears 1 time
@height9: 8px; // Appears 1 time

